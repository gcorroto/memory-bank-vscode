{
  "name": "grec0ai-vscode",
  "version": "3.0.0",
  "icon": "resources/icons/logo_transparent.png",
  "private": true,
  "displayName": "Grec0AI for Developers",
  "description": "The Grec0AI for Developers extension allows you to generate tests, fix issues, and analyze code quality directly in Visual Studio Code.",
  "license": "MIT",
  "publisher": "grec0ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/gcorroto/autofixer_extension.git"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Testing",
    "Linters",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "grec0ai-filesystem",
          "title": "Grec0AI",
          "icon": "resources/icons/mutuaLogo.png"
        }
      ]
    },
    "views": {
      "grec0ai-filesystem": [
        {
          "id": "grec0ai-filesystem-tree",
          "name": "Project Files"
        },
        {
          "id": "grec0ai-coverage-summary",
          "name": "Coverage Summary"
        },
        {
          "id": "grec0ai-coverage-details",
          "name": "Details"
        }
      ]
    },
    "commands": [
      {
        "command": "git.connectionSettings.initializeGitRepository",
        "title": "Initialize Git Repository",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.filesystem.refresh",
        "title": "Refresh File Tree",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      },
      {
        "command": "grec0ai.automaticTest",
        "title": "Automatic Test",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/frame_sink.png",
          "light": "resources/icons/light/frame_sink.png"
        }
      },
      {
        "command": "grec0ai.coverage.refresh",
        "title": "Refresh Coverage",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      },
      {
        "command": "grec0ai.coverage.details.refresh",
        "title": "Refresh Details",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      },
      {
        "command": "grec0ai.explain",
        "title": "Explain Code",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.fix",
        "title": "Fix with Grec0AI",
        "category": "Grec0AI"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "grec0ai.filesystem.refresh",
          "when": "view == grec0ai-filesystem-tree",
          "group": "navigation"
        },
        {
          "command": "grec0ai.automaticTest",
          "when": "view == grec0ai-filesystem-tree",
          "group": "navigation",
          "icon": "$(rocket)"
        },
        {
          "command": "grec0ai.coverage.refresh",
          "when": "view == grec0ai-coverage-summary",
          "group": "navigation"
        },
        {
          "command": "grec0ai.coverage.details.refresh",
          "when": "view == grec0ai-coverage-details",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "grec0ai.automaticTest",
          "group": "grec0ai"
        },
        {
          "command": "grec0ai.explain",
          "group": "grec0ai"
        },
        {
          "command": "grec0ai.fix",
          "group": "grec0ai"
        }
      ],
      "view/item/context": [
        {
          "command": "git.connectionSettings.initializeGitRepository",
          "when": "view == grec0ai-filesystem-tree",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Grec0AI",
      "properties": {
        "grec0ai.filesystem.projectFolder": {
          "type": "string",
          "default": "",
          "description": "Project folder relative to workspace root",
          "scope": "application"
        },
        "grec0ai.filesystem.excludePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "node_modules",
            "dist",
            ".git",
            "coverage"
          ],
          "description": "Patterns to exclude from file tree (in addition to .gitignore)",
          "scope": "application"
        },
        "grec0ai.test.framework": {
          "type": "string",
          "enum": [
            "jasmine",
            "jest",
            "mocha"
          ],
          "default": "jasmine",
          "description": "Test framework to use for generated tests",
          "scope": "application"
        },
        "grec0ai.test.coverage.minimumThreshold": {
          "type": "number",
          "default": 70,
          "description": "Minimum coverage threshold (percentage)",
          "scope": "application"
        },
        "git.connectionSettings.jira": {
          "type": "string",
          "default": "",
          "description": "Associated Jira ticket ID"
        },
        "git.connectionSettings.gitRepoUrl": {
          "type": "string",
          "default": "https://github.com/",
          "description": "Remote Git repository URL"
        },
        "git.connectionSettings.gitUsername": {
          "type": "string",
          "default": "",
          "description": "Git username for repository access"
        },
        "git.connectionSettings.gitToken": {
          "type": "string",
          "default": "",
          "description": "Git personal access token (PAT) for repository access"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "@types/async-lock": "^1.1.1",
    "@types/bluebird": "*",
    "@types/request": "*",
    "acorn": "^8.14.0",
    "async-lock": "^1.1.4",
    "bluebird": "^3.5.0",
    "immutable": "^4.0.0-rc.12",
    "java-parser": "^2.3.2",
    "request": "^2.81.0",
    "ts-log-debug": "^4.0.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "typescript": "^2.9.2",
    "vscode": "^1.1.21"
  }
}
