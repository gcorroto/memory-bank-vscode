{
  "name": "grec0ai-vscode",
  "version": "3.0.0",
  "icon": "resources/icons/logo_transparent.png",
  "private": true,
  "displayName": "Grec0AI for Developers",
  "description": "The Grec0AI for Developers extension allows you to generate tests, fix issues, and analyze code quality directly in Visual Studio Code.",
  "license": "MIT",
  "publisher": "grec0ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/gcorroto/autofixer_extension.git"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Testing",
    "Linters",
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "grec0ai-filesystem",
          "title": "Grec0AI",
          "icon": "resources/icons/logo_square_transparent.png"
        }
      ]
    },
    "views": {
      "grec0ai-filesystem": [
        {
          "id": "grec0ai-filesystem-tree",
          "name": "Project Files"
        },
        {
          "id": "grec0ai-coverage-summary",
          "name": "Coverage Summary"
        },
        {
          "id": "grec0ai-coverage-details",
          "name": "Details"
        }
      ]
    },
    "commands": [
      {
        "command": "git.connectionSettings.initializeGitRepository",
        "title": "Initialize Git Repository",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.filesystem.refresh",
        "title": "Refresh File Tree",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      },
      {
        "command": "grec0ai.automaticTest",
        "title": "Automatic Test",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/frame_sink.png",
          "light": "resources/icons/light/frame_sink.png"
        }
      },
      {
        "command": "grec0ai.coverage.refresh",
        "title": "Refresh Coverage",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      },
      {
        "command": "grec0ai.coverage.details.refresh",
        "title": "Refresh Details",
        "category": "Grec0AI",
        "icon": {
          "dark": "resources/icons/dark/refresh.svg",
          "light": "resources/icons/light/refresh.svg"
        }
      },
      {
        "command": "grec0ai.explain",
        "title": "Explain Code",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.fix",
        "title": "Fix with Grec0AI",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.openai.configure",
        "title": "Configure OpenAI API Key",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.vectra.indexProject",
        "title": "Index Project for RAG",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.ask.macgyver",
        "title": "Preguntar a MacGyver",
        "category": "Grec0AI"
      },
      {
        "command": "grec0ai.agent.generateTest",
        "title": "Generate Test (Agent)",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.agent.analyzeCode",
        "title": "Analyze Code (Agent)",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.agent.fixError",
        "title": "Fix Error (Agent)",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.agent.explain",
        "title": "Explain Code (Agent)",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.agent.showLogs",
        "title": "Show Agent Logs",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.agent.testReasoningModel",
        "title": "Test Reasoning Model",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.runAutofixer",
        "title": "Ejecutar Autofixer",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.showEventsViewer",
        "title": "Grec0AI: Mostrar Visor de Eventos y Cambios"
      },
      {
        "command": "grec0ai.clearEvents",
        "title": "Grec0AI: Limpiar Eventos"
      },
      {
        "command": "grec0ai.toggleTerminal",
        "title": "Grec0AI: Mostrar/Ocultar Terminal CLI"
      },
      {
        "command": "grec0ai.showFlowViewer",
        "title": "Mostrar Visualizador de Flujos",
        "category": "Grec0AI Agent"
      },
      {
        "command": "grec0ai.showConfigViewer",
        "title": "Mostrar Configurador Visual",
        "category": "Grec0AI Agent"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "grec0ai.filesystem.refresh",
          "when": "view == grec0ai-filesystem-tree",
          "group": "navigation"
        },
        {
          "command": "grec0ai.automaticTest",
          "when": "view == grec0ai-filesystem-tree",
          "group": "navigation",
          "icon": "$(rocket)"
        },
        {
          "command": "grec0ai.coverage.refresh",
          "when": "view == grec0ai-coverage-summary",
          "group": "navigation"
        },
        {
          "command": "grec0ai.coverage.details.refresh",
          "when": "view == grec0ai-coverage-details",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "grec0ai.automaticTest",
          "group": "grec0ai"
        },
        {
          "command": "grec0ai.explain",
          "group": "grec0ai"
        },
        {
          "command": "grec0ai.fix",
          "group": "grec0ai"
        },
        {
          "command": "grec0ai.ask.macgyver",
          "group": "grec0ai"
        },
        {
          "command": "grec0ai.agent.generateTest",
          "group": "grec0ai-agent"
        },
        {
          "command": "grec0ai.agent.analyzeCode",
          "group": "grec0ai-agent"
        },
        {
          "command": "grec0ai.agent.fixError",
          "group": "grec0ai-agent"
        },
        {
          "command": "grec0ai.agent.explain",
          "group": "grec0ai-agent"
        },
        {
          "command": "grec0ai.agent.testReasoningModel",
          "group": "grec0ai-agent"
        },
        {
          "command": "grec0ai.runAutofixer",
          "group": "grec0ai-agent"
        }
      ],
      "view/item/context": [
        {
          "command": "git.connectionSettings.initializeGitRepository",
          "when": "view == grec0ai-filesystem-tree",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Grec0AI",
      "properties": {
        "grec0ai.filesystem.projectFolder": {
          "type": "string",
          "default": "",
          "description": "Project folder relative to workspace root",
          "scope": "application"
        },
        "grec0ai.filesystem.excludePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "node_modules",
            "dist",
            ".git",
            "coverage"
          ],
          "description": "Patterns to exclude from file tree (in addition to .gitignore)",
          "scope": "application"
        },
        "grec0ai.test.framework": {
          "type": "string",
          "enum": [
            "jasmine",
            "jest",
            "mocha"
          ],
          "default": "jasmine",
          "description": "Test framework to use for generated tests",
          "scope": "application"
        },
        "grec0ai.test.coverage.minimumThreshold": {
          "type": "number",
          "default": 70,
          "description": "Minimum coverage threshold (percentage)",
          "scope": "application"
        },
        "grec0ai.openai.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key para acceder a los servicios de IA",
          "scope": "application"
        },
        "grec0ai.openai.model": {
          "type": "string",
          "enum": [
            "gpt-4.1-mini",
            "gpt-4.1-nano",
            "o3-mini",
            "o4-mini",
            "codex-mini-latest"
          ],
          "default": "gpt-4.1-mini",
          "description": "Modelo de OpenAI a utilizar para las consultas",
          "scope": "application"
        },
        "grec0ai.openai.models.planning": {
          "type": "string",
          "enum": [
            "gpt-4.1-mini",
            "gpt-4.1-nano",
            "o3-mini",
            "o4-mini"
          ],
          "default": "o4-mini",
          "description": "Modelo de OpenAI a utilizar para tareas de planificación",
          "scope": "application"
        },
        "grec0ai.openai.models.analysis": {
          "type": "string",
          "enum": [
            "gpt-4.1-mini",
            "gpt-4.1-nano",
            "o3-mini",
            "o4-mini"
          ],
          "default": "gpt-4.1-mini",
          "description": "Modelo de OpenAI a utilizar para tareas de análisis",
          "scope": "application"
        },
        "grec0ai.openai.models.codegen": {
          "type": "string",
          "enum": [
            "gpt-4.1-mini",
            "gpt-4.1-nano",
            "o3-mini",
            "o4-mini",
            "codex-mini-latest"
          ],
          "default": "gpt-4.1-mini",
          "description": "Modelo de OpenAI a utilizar para tareas de generación de código",
          "scope": "application"
        },
        "grec0ai.vectra.indexPath": {
          "type": "string",
          "default": "",
          "description": "Ruta personalizada para el índice Vectra (por defecto: .grec0ai/vectra-index)",
          "scope": "application"
        },
        "grec0ai.rag.similarityThreshold": {
          "type": "number",
          "default": 0.7,
          "description": "Umbral de similitud para consultas RAG (entre 0 y 1)",
          "scope": "application"
        },
        "grec0ai.rag.contextCount": {
          "type": "number",
          "default": 3,
          "description": "Número de fragmentos de contexto a recuperar para RAG",
          "scope": "application"
        },
        "grec0ai.autofixer.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable automatic execution of instructions from autofixer.md file on startup",
          "scope": "application"
        },
        "grec0ai.agent.maxReplanning": {
          "type": "number",
          "default": 5,
          "minimum": 0,
          "maximum": 10,
          "description": "Maximum number of times the agent can replan a task if it fails (0 disables replanning)",
          "scope": "application"
        },
        "grec0ai.agent.intelligentValidation": {
          "type": "boolean",
          "default": true,
          "description": "Use LLM for intelligent plan validation instead of rule-based validation",
          "scope": "application"
        },
        "grec0ai.agent.autoReplanning": {
          "type": "boolean",
          "default": true,
          "description": "Automatically replan tasks when they fail or produce suboptimal results",
          "scope": "application"
        },
        "git.connectionSettings.jira": {
          "type": "string",
          "default": "",
          "description": "Associated Jira ticket ID"
        },
        "git.connectionSettings.gitRepoUrl": {
          "type": "string",
          "default": "https://github.com/",
          "description": "Remote Git repository URL"
        },
        "git.connectionSettings.gitUsername": {
          "type": "string",
          "default": "",
          "description": "Git username for repository access"
        },
        "git.connectionSettings.gitToken": {
          "type": "string",
          "default": "",
          "description": "Git personal access token (PAT) for repository access"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p ./",
    "watch-tests": "tsc -p ./ --watch",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@types/async-lock": "^1.1.1",
    "@types/bluebird": "*",
    "@types/glob": "^8.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/request": "*",
    "acorn": "^8.14.0",
    "async-lock": "^1.1.4",
    "bluebird": "^3.5.0",
    "dagre": "^0.8.5",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3",
    "grec0ai-vscode": "file:",
    "immutable": "^4.0.0-rc.12",
    "java-parser": "^2.3.2",
    "openai": "^4.103.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.10.0",
    "request": "^2.81.0",
    "ts-log-debug": "^4.0.4",
    "vectra": "^0.11.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "@types/vscode": "^1.26.0",
    "@types/webpack-env": "^1.18.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.40.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.1"
  }
}
